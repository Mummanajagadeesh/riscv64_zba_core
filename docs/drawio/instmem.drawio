<mxfile host="app.diagrams.net">
  <diagram name="Instruction_Memory">
    <mxGraphModel dx="1200" dy="700" grid="1" gridSize="10"
      guides="1" tooltips="1" connect="1"
      arrows="1" fold="1" page="1"
      pageScale="1" pageWidth="1100" pageHeight="850">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- ================= OUTER BOX ================= -->
        <mxCell id="imem" value=""
          style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;fontStyle=1;"
          vertex="1" parent="1">
          <mxGeometry x="420" y="220" width="360" height="200" as="geometry"/>
        </mxCell>

        <!-- ================= LABEL BELOW ================= -->
        <mxCell id="imem_label" value="Instruction Memory"
          style="html=1;align=center;fontStyle=1;fontSize=14;"
          vertex="1" parent="1">
          <mxGeometry x="420" y="440" width="360" height="30" as="geometry"/>
        </mxCell>

        <!-- ================= INTERNAL BLOCK ================= -->
        <mxCell id="imem_array"
          value="Instruction Memory Array&#xa;32-bit Words&#xa;Index = PCF[31:2]"
          style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;"
          vertex="1" parent="1">
          <mxGeometry x="450" y="260" width="300" height="100" as="geometry"/>
        </mxCell>

        <!-- ================= INPUT (LEFT) ================= -->
        <mxCell id="PCF" value="PCF[63:0]"
          style="rounded=1;align=right;fillColor=#f8cecc;strokeColor=#b85450;"
          vertex="1" parent="1">
          <mxGeometry x="150" y="300" width="170" height="30" as="geometry"/>
        </mxCell>

        <!-- ================= OUTPUT (RIGHT) ================= -->
        <mxCell id="instruction" value="instruction[31:0]"
          style="rounded=1;align=left;fillColor=#fff2cc;strokeColor=#d6b656;"
          vertex="1" parent="1">
          <mxGeometry x="830" y="300" width="200" height="30" as="geometry"/>
        </mxCell>

        <!-- ================= EDGES (ORTHOGONAL, STRAIGHT) ================= -->

        <!-- input -->
        <mxCell id="e1" edge="1" parent="1"
          source="PCF" target="imem"
          style="edgeStyle=orthogonalEdgeStyle;
                 exitX=1;exitY=0.5;exitPerimeter=1;
                 entryX=0;entryY=0.5;entryPerimeter=1;">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- output -->
        <mxCell id="e2" edge="1" parent="1"
          source="imem" target="instruction"
          style="edgeStyle=orthogonalEdgeStyle;
                 exitX=1;exitY=0.5;exitPerimeter=1;
                 entryX=0;entryY=0.5;entryPerimeter=1;">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
